{
  "Comment": "Validaciones",
  "StartAt": "Recibir Estructura",
  "States": {
    "Recibir Estructura":{
      "Type": "Task",
      "Resource": "${TransformXSLFunctionArn}",
      "OutputPath": "$",
      "Next": "MapState"
    },
    "MapState":{
      "Type": "Map",
      "ItemsPath": "$",
      "MaxConcurrency": 10,
      "Iterator": {
          "StartAt": "Validate",
          "States": {
              "Validate": {
                  "Type": "Task",
                  "Resource": "${ValidationFunctionArn}",
                  "End": true
              }
          }
      },
      "End": true
  }
  }
}