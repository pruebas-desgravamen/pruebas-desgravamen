{
  "Comment": "Upload File",
  "StartAt": "Save Info",
  "States": {
    "Save Info":{
      "Type": "Parallel",
      "Next": "Send URL",
      "Branches": [
        {
          "StartAt": "Save registry to Dynamo",
          "States": {
            "Save registry to Dynamo":{
              "Type": "Task",
              "Parameters": {
                "input.$" : "$$.Execution.Input.input"
            },
              "Resource": "${FirstServCargaFunctionArn}",
              "End": true
            }
          }
        },
        {
          "StartAt": "Subtract",
          "States": {
            "Subtract": {
              "Type": "Task",
              "Resource": "${InFunctionArn}",
              "OutputPath": "$",
              "End": true
            }
          }
        }
      ]
    },
    "Send URL":{
      "Type": "Pass",
      "Parameters":{
        "url.$": "$"
      },
      "End": true
    }
  }
}