{
  "Comment": "Upload File",
  "StartAt": "Save Info",
  "States": {
    "Save Info":{
      "Type": "Task",
      "Resource": "arn:aws:states:::dynamodb:updateItem",
      "Parameters": {
        "TableName": "${TableName}",
        "Key":{
          "pk": {
            "S": "TRAMA"
          },
          "sk": {
            "S.$": "States.Format('{}#{}', $$.Execution.Input.input.poliza, $$.Execution.Input.input.canalDeVenta)"
          }
        },
        "ReturnValues": "ALL_NEW"
      },
      "End": true
    }
  }
}